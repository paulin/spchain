<h2>Solutions</h2>
<table>
    <tr>
        <th>
            Description
        </th>
    </tr>
    <% @solutions.each do |solution| %>
    <tr>
        <td>
            <%= solution.description %>
        </td>
    </tr>
    <% end %>
</table>
</br>
</br>
<h2>Problems</h2>
<table>
    <tr>
        <th>
            Description
        </th>
    </tr>
    <% @problems.each do |problem| %>
    <tr>
        <td>
            <%= problem.description %>
        </td>
    </tr>
    <% end %>
</table>
